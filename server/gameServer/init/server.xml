	<String id="dbName">root</String>
	<!--数据库密码-->
	<String id="dbPassword">root</String>
	<!--数据库编码-->
	<String id="dbEncode">utf-8</String>
	<!--游戏数据库地址-->
	<String id="gameDB">jdbc:mysql://127.0.0.1:3306/zsg_rc</String>
	<!--sever地址-->
	<String id="serverIP">tcp://127.0.0.1:8100</String>
	<!--日志地址-->
	<String id="logServerIP">tcp://127.0.0.1:8102</String>
	<!--bi地址-->
	<String id="biServerIP">tcp://127.0.0.1:8104</String>
	<!-- 日志服务器开关 -->
	<String id="logSwitch">false</String>
	<!-- BI服务器开关 -->
	<String id="biSwitch">false</String>
